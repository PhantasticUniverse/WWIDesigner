<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:constraints xmlns:ns2="http://www.wwidesigner.com/Constraints">
    <constraintsName>3 groups, 1-1/8" max hole spacing</constraintsName>
    <objectiveDisplayName>Grouped hole-position and hole size optimizer</objectiveDisplayName>
    <objectiveFunctionName>HoleGroupFromTopObjectiveFunction</objectiveFunctionName>
    <numberOfHoles>7</numberOfHoles>
    <constraint>
        <displayName>Bore length</displayName>
        <category>Hole position</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.1905</lowerBound>
        <upperBound>0.6985</upperBound>
    </constraint>
    <constraint>
        <displayName>Ratio, from splitting edge, of top-hole position to bore length</displayName>
        <category>Hole position</category>
        <type>DIMENSIONLESS</type>
        <lowerBound>0.25</lowerBound>
        <upperBound>1.0</upperBound>
    </constraint>
    <constraint>
        <displayName>Group 1 (Hole 7 (top), Hole 6, Hole 5) spacing</displayName>
        <category>Hole position</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.02032</lowerBound>
        <upperBound>0.028575</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 5 to Hole 4 distance</displayName>
        <category>Hole position</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.02032</lowerBound>
        <upperBound>0.06985</upperBound>
    </constraint>
    <constraint>
        <displayName>Group 2 (Hole 4, Hole 3, Hole 2) spacing</displayName>
        <category>Hole position</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.02032</lowerBound>
        <upperBound>0.028575</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 2 to Hole 1 (bottom) distance</displayName>
        <category>Hole position</category>
        <type>DIMENSIONAL</type>
        <lowerBound>5.08E-4</lowerBound>
        <upperBound>0.003175</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 7 (top) diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.0015875</lowerBound>
        <upperBound>0.0127</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 6 diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.003175</lowerBound>
        <upperBound>0.0127</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 5 diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.003175</lowerBound>
        <upperBound>0.0127</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 4 diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.003175</lowerBound>
        <upperBound>0.0127</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 3 diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.003175</lowerBound>
        <upperBound>0.0127</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 2 diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.0015875</lowerBound>
        <upperBound>0.0079375</upperBound>
    </constraint>
    <constraint>
        <displayName>Hole 1 (bottom) diameter</displayName>
        <category>Hole size</category>
        <type>DIMENSIONAL</type>
        <lowerBound>0.0015875</lowerBound>
        <upperBound>0.0079375</upperBound>
    </constraint>
</ns2:constraints>
